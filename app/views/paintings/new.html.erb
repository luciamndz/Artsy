<div class="container w-50 m-4">
<h1>Add a new painting</h1>
<%= simple_form_for (@painting) do |f| %>
  <%= f.input :title %>
  <%= f.input :width %>
  <%= f.input :height %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Add Painting", class:"btn btn-primary"%>
<% end %>
</div>
